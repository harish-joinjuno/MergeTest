{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "ext-simplexml": "*",
        "aws/aws-php-sns-message-validator": "^1.6",
        "aws/aws-sdk-php": "^3.112",
        "dillingham/nova-items-field": "^0.5.0",
        "doctrine/dbal": "^2.9",
        "drewm/mailchimp-api": "^2.5",
        "epartment/nova-dependency-container": "^1.2",
        "facade/ignition": "^2.0",
        "facebook/php-business-sdk": "8.0.0",
        "fideloper/proxy": "^4.2",
        "firebase/php-jwt": "^5.2",
        "fruitcake/laravel-cors": "^1.0",
        "google/apiclient": "^2.0",
        "googleads/google-ads-php": "^5.0",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.5",
        "helpscout/api": "^3.0",
        "hubspot/api-client": "^1.1",
        "jaybizzle/crawler-detect": "^1.2",
        "kabbouchi/nova-impersonate": "^1.3",
        "kamerk22/amazongiftcode": "^1.0",
        "laravel/framework": "^7.5",
        "laravel/horizon": "^4.0",
        "laravel/nova": "^3.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.1",
        "league/csv": "^9.6",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mailchimp/marketing": "^3.0",
        "nesk/puphpeteer": "^1.6",
        "optimistdigital/nova-multiselect-field": "^1.9",
        "optimistdigital/nova-sortable": "^1.3",
        "php-http/curl-client": "^1.7",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.2",
        "sentry/sentry-laravel": "^1.7",
        "simplesquid/nova-advanced-number-field": "^2.0",
        "symfony/dom-crawler": "^5.1",
        "theiconic/php-ga-measurement-protocol": "^2.7",
        "twilio/sdk": "^6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.9",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "^8.5",
        "sempro/phpunit-pretty-print": "^1.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish"
        ],
        "check": [
            "./vendor/bin/php-cs-fixer fix --dry-run --using-cache=no --verbose --stop-on-violation",
            "./vendor/bin/phpmd app text ./phpmd.xml",
            "./vendor/bin/phpcs --standard=phpcs.xml",
            "./node_modules/.bin/eslint --ext .js,.vue resources/js",
            "./vendor/bin/phpunit"
        ],
        "fix": [
            "./node_modules/.bin/eslint --ext .js,.vue resources/js --fix",
            "./vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ]
}
